{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/(tabs)/prompt.js",
    "app/(tabs)/index.js",
    "app/(tabs)/_layout.js",
    "app/modal.js",
    "app/+not-found.js",
    "app/+html.js",
    "app/_layout.js"
  ]
}
